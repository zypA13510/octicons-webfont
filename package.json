{
  "name": "octicons-webfont",
  "version": "0.0.0-development",
  "description": "An unofficial web-font port of Octicons",
  "scripts": {
    "commit": "git-cz",
    "lint:eslint:ci": "eslint . --ignore-path .gitignore --format junit --output-file .reports/eslint/junit.xml",
    "lint:eslint:local": "eslint . --ignore-path .gitignore",
    "lint:eslint": "if is-ci; then npm run lint:eslint:ci; else npm run lint:eslint:local; fi",
    "lint:commitlint": "commitlint -t HEAD",
    "lint": "npm-run-all --parallel lint:eslint lint:commitlint",
    "test:mocha:ci": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=.reports/mocha/junit.xml",
    "test:mocha:local": "mocha",
    "test:mocha": "if is-ci; then npm run test:mocha:ci; else npm run test:mocha:local; fi",
    "test": "npm-run-all --parallel test:*",
    "build": "grunt build",
    "semantic-release": "semantic-release"
  },
  "repository": "github:zypA13510/octicons-webfont",
  "keywords": [
    "octicons",
    "icons",
    "font",
    "web font",
    "icon font"
  ],
  "author": "Yuping Zuo (https://github.com/zypA13510)",
  "contributors": [
    "GitHub Inc. (https://octicons.github.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zypA13510/octicons-webfont/issues"
  },
  "homepage": "https://github.com/zypA13510/octicons-webfont#readme",
  "dependencies": {
    "@primer/octicons": "9.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.6",
    "@semantic-release/git": "^7.0.16",
    "autoprefixer": "^9.6.1",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.1.0",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "fontkit": "^1.8.0",
    "glob": "^7.1.4",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.5.0",
    "grunt-exec": "^3.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-webfont": "~1.5.0",
    "is-ci": "^2.0.0",
    "load-grunt-config": "^3.0.0",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^15.13.24"
  },
  "bundledDependencies": [
    "@primer/octicons"
  ],
  "files": [
    "build/**/*",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "browserslist": "defaults"
}
